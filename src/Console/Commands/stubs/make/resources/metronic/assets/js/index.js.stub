import React from 'react';
import {render, hydrate} from 'react-dom';
import DataTableComponent from '../components/DataTableComponent';

let defColumns = [{
    field: 'id',
    title: '#',
    sortable: false,
    width: 40,
    selector: false,
    textAlign: 'center',
    searchable: true,
}];

hydrate(
    <DataTableComponent
        restful={true}
        resource='DummyResource'
        defColumns={defColumns}
    />,
    document.getElementById("DummyTableId"),
);
