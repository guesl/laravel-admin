{
    "name": "guesl/laravel-admin",
    "version": "v1.0.0",
    "description": "Laravel admin wrote by Jianfeng Li, Guesl.",
    "keywords": [
        "admin",
        "laravel",
        "ACL"
    ],
    "license": "MIT",
    "homepage": "https://github.com/guesl/laravel-admin",
    "support": {
        "issues": "https://github.com/guesl/laravel-admin/issues",
        "source": "https://github.com/guesl/laravel-admin"
    },
    "authors": [
        {
            "name": "Jianfeng Li",
            "email": "modifyljf@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.5"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Guesl\\Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guesl\\Admin\\Providers\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Guesl\\Admin\\Facades\\Admin"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
